    <!-- my calls -->
    <script>
      new WOW().init();
      $(function () {
        initMap();
        let isLogin = false;
        // If not logged in, redirect to loginot make account first before any operation
        $("body").on("click", function () {
          if (!isLogin) {
            checkLoginStatus().then((data) => {
              if (!data) document.location.href = "login";
              else isLogin = true;
            });
          }
        });
      });
      /**
       * @param - event (e)
       * Prompt user to login/register if not
       */
      const socket = io("http://localhost:9000/");
      socket.on("login", (m) => console.log(m));
      // function onMapClick(e) {
      //   checkLoginStatus().then((data) => {
      //     if (!data) document.location.href = "login";
      //   });
      // }

      getLatLon((err, result) => {
        if (result) {
          getAddressDetails(result[0], result[1], (err, res) => {
            $("[data-country]").text(res.country);
            $("[data-state]").text(res.state);
            $("[data-city]").text(res.city);
            // Location counters
            dob = fetchDob();
            fetchLocationCounts(
              res.country || "",
              res.state || "",
              res.city || "",
              dob || ""
            ).then((data) => {
              $("[data-country-count]").text(data.countryCount || 0);
              $("[data-state-count]").text(data.stateCount || 0);
              $("[data-city-count]").text(data.cityCount || 0);
            });
          });
        }
      });
      //   mymap.on("click", onMapClick);

      let page = 1;
      let perPage = 10;
      checkLoginStatus().then((data) => {
        if (data === true) {
          // Fetch suggestions
          fetchSuggestions(page, perPage).then((myusers) => {
            const total = myusers.total;
            const users = myusers.data;
            const { maleCount, femaleCount } = myusers;

            $("[data-world-count]").text(total);
            $("[data-all-users]").text(total);
            showPieChart(maleCount, femaleCount);
            if (users && users.length) {
              showUsersToConnect(users);
            } else {
              $("#user-grid").append("<p>No more suggestion available.</p>");
              page = 0;
            }
          });

          // Infinite scroll
          const infiniteScroll = () => {
            let isFetching = false;
            if (checkIfAtBottom()) {
              // add something end of page
              if (page > 0 && !isFetching) {
                page++;
                isFetching = true;
                $("#user-grid").append(loader);
                fetchSuggestions(page, perPage).then((myusers) => {
                  $("#user-grid .loader-bars").remove();
                  const users = myusers.data;
                  if (users && users.length) {
                    showUsersToConnect(users);
                  } else {
                    page = 0;
                    $("#user-grid .loader-bars").remove();
                    $("#user-grid").append(
                      "<p>No more suggestion available.</p>"
                    );
                  }
                  isFetching = false;
                });
              }
            }
          };
          $(window).on("scroll", infiniteScroll); // DESKTOP
          $("html,body").on("touchmove", infiniteScroll); // TOUCH MOBILE

          // Common data
          fetchCommonData().then((data) => {
            // If not authorized then login else fetch
            let avatar = data.avatar
              ? getFullUrl(data.avatar)
              : "img/custom/nouser.png";
            let cover = data.cover
              ? getFullUrl(data.cover)
              : "img/custom/nocover.png";
            if (data && data.status === ERRORS.AUTH_ERROR) {
              redirectTo("login");
            } else {
              $(".profile-image img").attr("src", avatar);
              $(".coverImage").attr("src", cover);
              $("[data-profile-completed]").text(
                Math.round(data.profilePercentage) + "%"
              );
              $("[data-user-name]").text(data.name);
              $("title").text("Welcome " + data.name);
              $("[data-connects]").text(data.connectionCount);
              $("[data-username]").text("@" + data.username);
              $("[data-description]").text(data.description);
              $("[data-last-login]").text(
                data.lastLogin
                  ? moment(data.lastLogin).calendar()
                  : "It's Your First Time ðŸ˜„"
              );
            }
          });
        } else {
          // *NOT LOGGED IN
          $(".sidemenu-trigger").hide();
          fetchUsers(page, perPage).then((myusers) => {
            const total = myusers.total;
            const users = myusers.data;
            const { maleCount, femaleCount } = myusers;

            $("[data-world-count]").text(total);
            $("[data-all-users]").text(total);
            showPieChart(maleCount, femaleCount);
            if (users && users.length) {
              showUsersToConnect(users);
            } else {
              $("#user-grid").append("<p>No suggestion available.</p>");
              page = 0;
            }
          });

          // Infinite scroll
          const infiniteScroll = () => {
            let isFetching = false;
            if (checkIfAtBottom()) {
              // add something end of page
              if (page > 0 && !isFetching) {
                page++;
                let isFetching = true;
                $("#user-grid").append(loader);
                fetchUsers(page, perPage).then((myusers) => {
                  $("#user-grid .loader-bars").remove();
                  const users = myusers.data;
                  if (users && users.length) {
                    showUsersToConnect(users);
                  } else {
                    // Stop fetching more
                    page = 0;
                    $("#user-grid").append("<p>No suggestion available.</p>");
                  }
                  isFetching = false;
                });
              }
            }
          };
          $(window).on("scroll", infiniteScroll); // DESKTOP
          $("html,body").on("touchmove", infiniteScroll); // TOUCH MOBILE
        }
      });

      // LOAD LEADERBOARD
      fetchLeaderboard().then((leaderboard) => {
        leaderboard.forEach((item, i) => {
          $("#leaderboard").append(`
                <tr>
                  <td>${i + 1}</td>
                  <td>${moment(item.dob).format("MMM Do YY")}</td>
                  <td>${item.total}</td>
                  <td>${item.connects}</td>
                </tr>
              `);
        });
      });
    </script>
