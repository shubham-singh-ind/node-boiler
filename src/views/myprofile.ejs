            <div class="widget-box-content">
              <!-- TIMELINE INFORMATION LIST -->
              <div class="timeline-information-list" id="education-list">
                <!-- *EDUCATION ITEMS WILL GO HERE -->
              </div>
              <!-- /TIMELINE INFORMATION LIST -->
            </div>
            <!-- /WIDGET BOX CONTENT -->
          </div>
          <!-- /WIDGET BOX -->
        </div>
        <!-- /GRID COLUMN -->
      </div>
      <!-- /GRID -->
    </div>
    <!-- /CONTENT GRID -->

    <%- include('./reuseables/commonIncludes.ejs') -%>

    <!-- JQUERY -->
    <script src="./myjs/jquery.min.js"></script>
    <!-- CUSTOM JS -->
    <script src="./myjs/main.js"></script>
    <!-- CUSTOM JS -->
    <script src="./myjs/logout.js"></script>
    <!-- MOMENT JS -->
    <script src="./js/moment/moment.js"></script>
    <!-- SIDEBAR DATA JS -->
    <script src="./myjs/sidebardata.js"></script>

    <script>
      const showPersonalInfo = (data) => {
        $("[data-email]").text(data.email);
        $("[data-dob]").text(moment(data.dob).format("LL"));
        $("[data-profession]").text(data.profession || "Not available");
        $("[data-marital]").text(data.maritalStatus || "Not available");
        $("[data-address]").text(data.address || "Not available");
        $("[data-age]").text(calculateAge(data.dob) || "Not available");
        const country = data.country ? data.country.split(" ")[0] : "";
        $("[data-country]").text(country || "Not available");
        $("[data-city]").text(data.city || "Not available");
        $("[data-flag]").attr("src", `img/flag/${country.toLowerCase()}.png`);
        $("[data-website]").append(
          data.website
            ? `<a href="${data.website}">${data.website}</a>`
            : "Not available"
        );
        $("[data-user-website]").append(
          data.website
            ? `<a href="${data.website}">${data.website}</a>`
            : "Not available"
        );
      };

      const showInterests = (interests) => {
        $("[data-tv-shows]").text(interests.tvShows || "Not available");
        $("[data-music-bands-artists]").text(
          interests.musicBandsArtists || "Not available"
        );
        $("[data-movies]").text(interests.movies || "Not available");
        $("[data-books]").text(interests.books || "Not available");
        $("[data-games]").text(interests.games || "Not available");
        $("[data-hobbies]").text(interests.hobbies || "Not available");
        $("[data-food-habits]").text(interests.foodHabits || "Not available");
      };

      const showEducations = (educations) => {
        const educationList = $("#education-list");
        if (!educations || !educations.length) {
          educationList.append(
            '<p class="information-block-text" data-food-habits="">Not available</p>'
          );
          return;
        }
        for (let education of educations) {
          const item = educationItem(education);
          educationList.append(item);
        }
      };

      fetchSelfProfile().then((data) => {
        // If not authorized then login else fetch
        if (data && data.status === ERRORS.AUTH_ERROR) {
          redirectTo("login");
        } else {
          console.log(data);
          const avatar = data.avatar
            ? getFullUrl(data.avatar)
            : "./img/custom/nouser.png";
          $("[data-user-name]").text(data.name);
          $("title").text(data.name + "'s | Profile");
          $(".profile-image img").attr("src", avatar);
          const joinedAt = moment(data.createdAt).format("LL");
          $("[data-joined]").text(joinedAt);
          $("[data-description]").text(data.description);
          $("[data-friend-count]").text(data.friendCount);
          showSocialAccounts(data.socialAccounts);
          showPersonalInfo(data);
          showInterests(data.interests);
          showEducations(data.educations);
        }
      });
    </script>
  </body>
</html>
