<script>
  const setPrivacySettings = (privacies) => {
    privacies.forEach(privacy => {
      $(`select[name=${privacy.privacyColumn}]`).val(privacy.privacyType);
    })
  }

  fetchPrivacyDetails()
  .then(data => {
    // If not authorized then login else fetch
    if (data && data.status === ERRORS.AUTH_ERROR) {
      redirectTo("login");
    } else {
      const {privacies, socialPrivacies} = data;
      // set privacy details here
      setPrivacySettings(privacies);
      setPrivacySettings(socialPrivacies);
    }
  })
</script>

